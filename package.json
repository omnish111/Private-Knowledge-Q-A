{
  "name": "private-qa-app",
  "version": "1.0.0",
  "description": "Private Q&A Application",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=private-qa-app/server\" \"npm run dev --workspace=private-qa-app/client\"",
    "server": "npm run dev --workspace=private-qa-app/server",
    "client": "npm run dev --workspace=private-qa-app/client",
    "install-all": "npm install && npm install --workspace=private-qa-app/server && npm install --workspace=private-qa-app/client"
  },
  "workspaces": [
    "private-qa-app/server",
    "private-qa-app/client"
  ],
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
